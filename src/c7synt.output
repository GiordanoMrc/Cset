   16                  | %empty

   17 stmts: stmts stmt
   18      | stmt
   19      | error

   20 stmt: io-stmt
   21     | return-stmt
   22     | compound-stmt
   23     | if-stmt
   24     | for-stmt
   25     | expression-stmt
   26     | forall-stmt
   27     | var-declaration

   28 io-stmt: READ '(' expression ')' ';'
   29        | WRITE '(' expression ')' ';'
   30        | WRITELN '(' expression ')' ';'

   31 if-stmt: IF '(' expression ')' stmt
   32        | IF '(' expression ')' stmt ELSE stmt
   33        | IF '(' expression IN expression ')' expression-stmt ELSE expression-stmt

   34 for-stmt: FOR '(' expression ';' expression ';' expression ')' stmt ';'

   35 return-stmt: RETURN expression ';'

   36 forall-stmt: FORALL '(' in-exp ')' compound-stmt
   37            | FORALL '(' in-exp ')' expression-stmt

   38 expression-stmt: expression ';'
   39                | ';'

   40 expression: assign-exp
   41           | expression ',' assign-exp

   42 assign-exp: basic-exp
   43           | basic-exp EQ assign-exp

   44 in-exp: expression IN in-exp
   45       | expression

   46 basic-exp: logical-exp
   47          | logical-exp OR logical-exp
   48          | logical-exp AND logical-exp
   49          | NOT logical-exp

   50 logical-exp: add-exp
   51            | add-exp EQ_TO add-exp
   52            | add-exp NEQ_TO add-exp
   53            | add-exp GT add-exp
   54            | add-exp LT add-exp
   55            | add-exp GTE add-exp
   56            | add-exp LTE add-exp

   57 add-exp: term
   58        | term PLUS term
   59        | term MINUS term

   60 term: factor
   61     | term MULT factor
   62     | term DIV factor

   63 factor: '(' expression ')'
   64       | ID
   65       | constant
   66       | call
   67       | set-exp

   68 set-exp: ADD '(' in-exp ')'
   69        | REMOVE '(' in-exp ')'
   70        | EXISTS '(' in-exp ')'
   71        | IS_SET '(' in-exp ')'

   72 constant: INTEGER_CONST
   73         | FLOAT_CONST
   74         | EMPTY_CONST
   75         | STRING

   76 call: ID '(' arg-list ')'
   77     | ID '(' ')'

   78 arg-list: factor
   79         | arg-list ',' factor


